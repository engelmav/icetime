{
    "name": "saasterkit",
    "version": "1.2.0",
    "private": true,
    "scripts": {
        "dev": "npm run prisma-generate && next dev",
        "build": "npx prisma generate && next build",
        "start": "npm run prisma-generate && next start",
        "lint": "npm run prisma-generate && next lint",
        "prisma-generate": "dotenv -e ${ENV_FILE:-.env.local} -- npx prisma generate",
        "prisma-studio": "dotenv -e ${ENV_FILE:-.env.local} -- npx prisma studio",
        "prisma-migrate": "dotenv -e ${ENV_FILE:-.env.local} -- npx prisma migrate dev",
        "prisma-reset": "dotenv -e ${ENV_FILE:-.env.local} -- npx prisma migrate reset --force",
        "prisma-seed": "dotenv -e ${ENV_FILE:-.env.local} -- npx prisma db seed",
        "docker:up": "docker-compose up -d",
        "docker:down": "docker-compose down",
        "docker:dev": "cp .env.local.docker .env.local && dotenv -e .env.local -- npm run prisma-migrate && dotenv -e .env.local -- npm run dev",
        "docker:clean": "docker-compose down -v",
        "prisma-generate:prod": "ENV_FILE=.env.production npm run prisma-generate",
        "prisma-studio:prod": "ENV_FILE=.env.production npm run prisma-studio",
        "prisma-migrate:prod": "ENV_FILE=.env.production npm run prisma-migrate",
        "prisma-reset:prod": "ENV_FILE=.env.production npm run prisma-reset",
        "prisma-seed:prod": "ENV_FILE=.env.production npm run prisma-seed"
    },
    "prisma": {
        "schema": "src/libs/database/schema.prisma",
        "seed": "ts-node --compiler-options {\"module\":\"CommonJS\"} src/libs/database/seed.ts"
    },
    "dependencies": {
